import boto3
import json

# Replace with your real endpoint name from the console/logs
endpoint_name = "sagemaker-scikit-learn-2025-06-03-..."  

runtime = boto3.client("sagemaker-runtime")

# This dummy input must match what your model expects (5 features for now)
input_data = [0.2, 0.4, 0.6, 0.1, 0.9]

response = runtime.invoke_endpoint(
    EndpointName=endpoint_name,
    ContentType="application/json",
    Body=json.dumps(input_data)
)

result = response['Body'].read().decode()
print("Prediction:", result)